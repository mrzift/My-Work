//---Factorial Calculator---//

        Scanner sc = new Scanner(System.in);
        
        // Taking input from user.
        int n= sc.nextInt();
        
        int f=1;
        for (int i=0; i<n; i++){
            f= f*(n-i);
        }
        System.out.println(f);
        
        
  // Using Recursion //
public static int fact(int n){
        if (n==0){
        return 1;
        }
        return n*fact(n-1);
 }
