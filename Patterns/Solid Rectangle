 //---Solid Rectangle---//
 
        Scanner sc = new Scanner(System.in);
        System.out.println("Enter the number of rows");
        int row = sc.nextInt();
        System.out.println("Enter the number of columns");
        int colm = sc.nextInt();
        for (int i = 1; i <= row; i++) {
            for (int j = 1; j <= colm; j++) {
                System.out.print("* ");
            }
            System.out.println();
        }
